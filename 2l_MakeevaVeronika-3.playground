// Создать возрастающий массив из 100 чисел.

func generateOrderedRandomIntArray(ArraySize arraySize: Int, StartNumber startNumber: Int, EndNumber endNumber: Int) -> Array<Int> {
    var arr = [Int]()
    for _ in 1...arraySize {
        arr.append(Int.random(in: startNumber...endNumber))
    }
    return arr.sorted(by: >)
}

var start: Int = 5
var end: Int = 10
var size: Int = 10
var arr = generateOrderedRandomIntArray(ArraySize: size, StartNumber: start, EndNumber: end)

print(arr)
